index d6d9dc2..58439d9 100644
--- a/agent/reconciler/host_reconciler.go
+++ b/agent/reconciler/host_reconciler.go
@@ -15,6 +15,7 @@ import (
 	corev1 "k8s.io/api/core/v1"
 	"k8s.io/apimachinery/pkg/types"
 	"k8s.io/client-go/tools/record"
+	"k8s.io/client-go/util/retry" // <--- ADD THIS LINE
 	clusterv1 "sigs.k8s.io/cluster-api/api/v1beta1"
 	"sigs.k8s.io/cluster-api/util/conditions"
 	"sigs.k8s.io/cluster-api/util/patch"
@@ -141,6 +142,56 @@ func (r *HostReconciler) reconcileNormal(ctx context.Context, byoHost *infrastru
 			conditions.MarkFalse(byoHost, infrastructurev1beta1.K8sNodeBootstrapSucceeded, infrastructurev1beta1.CloudInitExecutionFailedReason, clusterv1.ConditionSeverityError, "")
 			return ctrl.Result{}, err
 		}
+		// --- START OF NEW CODE ---
+
+		// After bootstrapping, patch the associated ByoMachine with the ProviderID and Status
+		// This is the missing step that causes Machines to get stuck in "Provisioning"
+		logger.Info("Bootstrapping successful, now patching ByoMachine", "ByoMachineName", byoHost.Status.MachineRef.Name)
+		
+		byoMachine := &infrastructurev1beta1.ByoMachine{}
+		byoMachineKey := client.ObjectKey{
+			Namespace: byoHost.Status.MachineRef.Namespace,
+			Name:      byoHost.Status.MachineRef.Name,
+		}
+
+		// 1. Patch Spec (ProviderID) with Retries
+		if err := retry.RetryOnConflict(retry.DefaultRetry, func() error {
+			// Get the latest version of the ByoMachine
+			if err := r.Client.Get(ctx, byoMachineKey, byoMachine); err != nil {
+				logger.Error(err, "failed to get ByoMachine for providerID patch")
+				return err
+			}
+			// Apply the change
+			patch := client.MergeFrom(byoMachine.DeepCopy())
+			byoMachine.Spec.ProviderID = "byoh://" + byoHost.Name // The ProviderID is the ByoHost's name
+			// Use r.Client.Patch for the spec
+			return r.Client.Patch(ctx, byoMachine, patch)
+		}); err != nil {
+			// If this fails, return the error so we retry the whole reconcile
+			return ctrl.Result{}, errors.Wrap(err, "failed to patch byomachine providerID after retries")
+		}
+		logger.Info("Successfully patched ByoMachine providerID")
+
+
+		// 2. Patch Status (Provisioned) with Retries
+		if err := retry.RetryOnConflict(retry.DefaultRetry, func() error {
+			// Get the latest version of the ByoMachine again
+			if err := r.Client.Get(ctx, byoMachineKey, byoMachine); err != nil {
+				logger.Error(err, "failed to get ByoMachine for status patch")
+				return err
+			}
+			// Apply the change
+			patch := client.MergeFrom(byoMachine.DeepCopy())
+			byoMachine.Status.Ready = true
+			// Use r.Client.Status().Patch for the status
+			return r.Client.Status().Patch(ctx, byoMachine, patch)
+		}); err != nil {
+			// If this fails, return the error so we retry the whole reconcile
+			return ctrl.Result{}, errors.Wrap(err, "failed to patch byomachine status after retries")
+		}
+		logger.Info("Successfully patched ByoMachine status as provisioned")
+        
+// --- END OF NEW CODE ---
 		logger.Info("k8s node successfully bootstrapped")
 		r.Recorder.Event(byoHost, corev1.EventTypeNormal, "BootstrapK8sNodeSucceeded", "k8s Node Bootstraped")
 		conditions.MarkTrue(byoHost, infrastructurev1beta1.K8sNodeBootstrapSucceeded)
